{

"manifest_version": 2,
"name": "Trello Example",
"description": "This will hopefully allow you to popout trello lists",
"version": "0.1",

"permissions": [
		"activeTab",
		"https://ajax.googleapis.com/"
	],

"browser_action": {
	"default_icon": "icon.png",
	"default_popup": "popup.html"
	},
	

	"content_scripts": [
		{
        "matches": ["https://*.trello.com/*"],
        "js": ["trello_list.js"]
		}
	]
}